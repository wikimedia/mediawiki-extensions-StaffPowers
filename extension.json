{
	"name": "StaffPowers",
	"version": "1.5",
	"author": [
		"Åukasz Garczewski",
		"Jack Phoenix",
		"Mainframe98"
	],
	"license-name": "GPL-2.0-or-later",
	"url": "https://www.mediawiki.org/wiki/Extension:StaffPowers",
	"descriptionmsg": "staffpowers-desc",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"MessagesDirs": {
		"StaffPowers": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\StaffPowers\\": "includes/"
	},
	"Hooks": {
		"BlockIp": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\StaffPowers\\Hooks",
			"services": [
				"UserFactory",
				"UserNameUtils",
				"UserGroupManager",
				"HookContainer"
			]
		}
	},
	"AvailableRights": [
		"unblockable"
	],
	"GroupPermissions": {
		"staff": {
			"unblockable": true
		}
	},
	"config": {
		"StaffPowersStewardGroupName": {
			"description": "Internal name of the 'steward' user group",
			"value": "steward"
		},
		"StaffPowersShoutWikiMessages": {
			"description": "Whether to use ShoutWiki-specific i18n messages (true) or not (false)",
			"value": true
		}
	},
	"manifest_version": 2
}
